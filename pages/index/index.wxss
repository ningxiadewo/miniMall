.index swiper {
  width: 750rpx;
  height: 340rpx;
}
.index swiper swiper-item image {
  width: 100%;
}
.index .index_cates {
  display: flex;
  padding: 10rpx;
}
.index .index_cates navigator {
  flex: 1;
}
.index .index_floor .wrapper .floor_title {
  padding-top: 20rpx;
}
.index .index_floor .wrapper .floor_title image {
  width: 100%;
}
.index .index_floor .wrapper .floor_list {
  overflow: hidden;
}
.index .index_floor .wrapper .floor_list navigator {
  float: left;
  width: 33.33%;
}
.index .index_floor .wrapper .floor_list navigator:nth-last-child(-n + 4) {
  height: 27.72711207vw;
  padding-left: 10rpx;
}
.index .index_floor .wrapper .floor_list navigator:nth-last-child(-n + 2) {
  padding-top: 10rpx;
}
.index .index_floor .wrapper .floor_list navigator image {
  width: 100%;
  height: 100%;
  vertical-align: middle;
}
